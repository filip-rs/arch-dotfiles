// LAPTOP CONFIG


[{
    "layer": "top",
    "output": "eDP-1",
    "position": "bottom",
    "mod": "dock",
    "exclusive": true,
    "passthrough": false,
    "gtk-layer-shell": true,
    "height": 0,
    "modules-left": ["clock", "custom/weather", "hyprland/workspaces", "custom/wallpaper-engine"],
    //"modules-center": ["hyprland/window"],
    "modules-right": ["tray","pulseaudio","pulseaudio#microphone","backlight","battery","custom/laptopwlogout"],
    "hyprland/window": {
        "format": "{}"
    },

   "hyprland/workspaces": {
        "disable-scroll": true,
        "on-scroll-up": "hyprctl dispatch workspace -1",
        "on-scroll-down": "hyprctl dispatch workspace +1",
		"format": "{name}: {windows}",
		"on-click": "activate",
		"format-icons": {
			"urgent": "ÔÅ™",
			"active": "",
			"default": ""
		},

    	"persistent-workspaces": {
    	         "*": 5
        },

        "window-rewrite-default": "Û±ìª",
	    "window-rewrite": {
		    "title<.*youtube.*>": "ÔÖ™", // Windows whose titles contain "youtube"
		    "class<firefox>": "Ôâ©", // Windows whose classes are "firefox"
		    "class<none>": "r", // Windows whose classes are "firefox"
		    "class<librewolf>": "Û∞àπ", // Windows whose classes are "firefox"
		    "class<librewolf> title<.*github.*>": "ÔÑì", // Windows whose class is "firefox" and title contains "github". Note that "class" always comes first.
		    "alacritty": "Óûï", // Windows that contain "foot" in either class or title. For optimization reasons, it will only match against a title if at least one other window explicitly matches against a title.
		    "thunar": "Û±ß∂", // Windows that contain "foot" in either class or title. For optimization reasons, it will only match against a title if at least one other window explicitly matches against a title.
		    "code": "Û∞®û",
		    "slack": "Ó¢§",
		    "youtube-music": "Û∞éÜ",
            "notion": "Ó°à",
		    "title<.* - (.*) - VSCodium>": "codium $1"  // captures part of the window title and formats it into output
	    }
    },

    "custom/power_profile":{
        //shows the current power profile and switches to next on click
        "exec": "asusctl profile -p | sed s:'Active profile is'::",
        "interval": 30,
        "format": "Û∞àê{}", 
        "on-click": "asusctl profile -n; pkill -SIGRTMIN+8 waybar",
        "signal": 8
    },

   "custom/weather" : {
        //shows the current weather and forecast
        "tooltip" : true,
        "format" : "{}",
        "interval" : 30,
        "exec" : "python ~/.config/waybar/scripts/waybar-wttr.py",
        "return-type" : "json"
    },

    "custom/laptopwlogout": {
	"format" : "{icon} ",
	"on-click" : "exec wlogout -b 5",
	"tooltip" : false,
	"format-icons": ["ÔÄë"]
    },


    "custom/wallpaper-engine": {
        "exec": "/usr/bin/dwall -s cliffs >/dev/null 2>&1",
        "interval": 60
    },


    "tray": {
        "icon-size": 18,
        "spacing": 10
    },

    "clock": {
        "format": "{:%H:%M %b %e}",
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>"
    },

    "backlight": {
        "device": "intel_backlight",
        "format": "{icon}   {percent}%",
        "format-icons": ["Û∞Éû", "Û∞Éü", "Û∞É†"],
        "on-scroll-up": "brightnessctl set 1%-",
        "on-scroll-down": "brightnessctl set 1%+",
        "min-length": 6
    },

    "battery": {
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 20
        },
        "tooltip": false,
        "format": "{icon} {capacity}%",
        "format-charging": "Ôá¶ {capacity}%",
        "format-plugged": "Ôá¶ {capacity}%",
        "format-alt": "{time} {icon}",
        "format-icons": ["Û∞Çé", "Û∞Å∫", "Û∞Åª", "Û∞Åº", "Û∞ÅΩ", "Û∞Åæ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞ÇÇ", "Û∞Åπ"]
    },

    "pulseaudio": {
        "format": "{icon}  {volume}%",
        "tooltip": false,
        "format-muted": "ÔÄ¶  Muted",
        "on-click": "pamixer -t",
        "on-click-right": "./usr/bin/pavucontrol",
        "on-scroll-up": "pamixer -d 1",
        "on-scroll-down": "pamixer -i 1",
        "scroll-step": 5,
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "ÔÄ•",
            "headset": "ÔÄ•",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        }
    },

    "pulseaudio#microphone": {
        "format": "{format_source}",
        "format-source": "ÔÑ∞  {volume}%",
        "format-source-muted": "ÔÑ±    Muted",
        "on-click": "pamixer --default-source -t",
        "on-scroll-up": "pamixer --default-source -d 1",
        "on-scroll-down": "pamixer --default-source -i 1",
        "scroll-step": 5
    },
    
    "temperature": {
        "thermal-zone": 1,
        "format": "{temperatureC}¬∞C Ôãâ",
        "critical-threshold": 80,
        "format-critical": "{temperatureC}¬∞C Ôãá"
    },

    "network": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "format-wifi": "Ôá´  {signalStrength}%",
        "format-ethernet": "{ipaddr}/{cidr}",
        "tooltip-format": "{essid} - {ifname} via {gwaddr}",
        "format-linked": "{ifname} (No IP)",
        "format-disconnected": "Disconnected ‚ö†",
        "format-alt": "{ifname}:{essid} {ipaddr}/{cidr}"
    },

    "bluetooth": {
	    "format": "Ôäî {status}",
	    "format-disabled": "", // an empty format will hide the module
	    "format-connected": "Ôäî {num_connections}",
	    "tooltip-format": "{device_alias}",
	    "tooltip-format-connected": "Ôäî {device_enumerate}",
	    "tooltip-format-enumerate-connected": "{device_alias}"
    }
